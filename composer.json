{
    "name": "wecode/website-creation-helper",
    "description": "Modern website creation helper with Twig templates and popular CSS frameworks",
    "type": "library",
    "license": "MIT",
    "homepage": "https://wecode.swiss",
    "support": {
        "issues": "https://github.com/wecodegeneva/website-creation-helper/issues",
        "source": "https://github.com/wecodegeneva/website-creation-helper"
    },
    "authors": [
        {
            "name": "WecodeGeneva",
            "email": "bruno@wecode.swiss",
            "homepage": "https://wecode.swiss"
        }
    ],
    "require": {
        "php": "^8.0",
        "twig/twig": "^3.0",
        "symfony/console": "^6.0",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Wecode\\WebsiteCreationHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wecode\\WebsiteCreationHelper\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["bin/wch"]
}
